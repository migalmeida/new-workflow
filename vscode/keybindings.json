// Place your key bindings in this file to overwrite the defaults
[
    { "key": "ctrl+l",         "command": "workbench.action.nextEditor" },
    { "key": "ctrl+h",         "command": "workbench.action.previousEditor" },
    { "key": "ctrl+w ctrl+w",         "command": "workbench.action.nextEditor" },
    { "key": "ctrl+j",         "command": "workbench.action.terminal.focus" },
    { "key": "ctrl+k",         "command": "workbench.action.terminal.toggleTerminal" },
    { "key": "cmd+r",          "command": "workbench.action.openRecent"}
]